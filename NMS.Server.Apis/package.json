{
    "name": "nms-server-apis",
    "version": "0.0.1",
    "description": "Nms Project for apis",
    "main": "server.ts",
    "scripts": {
        "build": "tsc",
        "debug": "nodemon --nolazy --inspect-brk=8080 src/**/*.* --exec ts-node src/server.ts",
        "dev": "nodemon --watch src/**/*.* --exec ts-node src/server.ts",
        "start": "nodemon --exec ts-node src/server.ts",
        "server": "ts-node -r tsconfig-paths/register src/app.ts",
        "prod": "npm run build && npm run start",
        "test": "echo \"Error: no test specified\" && exit 1",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tslint -p tsconfig.json"
    },
    "author": {
        "name": "Raza Sultan",
        "email": "rxzx.sultxn@gmail.com"
    },
    "dependencies": {
        "@types/node": "^11.9.5",
        "body-parser": "^1.15.2",
        "express": "^4.14.0",
        "minimist": "*",
        "mssql": "^3.3.0",
        "nodemon": "^1.18.10",
        "npm-run-all": "^4.1.5",
        "request": "^2.88.0",
        "rimraf": "^2.6.3",
        "swagger-node-express": "~2.0",
        "xml-js": "^1.6.11"
    },
    "devDependencies": {
        "@types/express": "^4.16.1",
        "@types/mssql": "^4.0.12",
        "body-parser": "^1.18.3",
        "express": "^4.16.4",
        "node": "^11.1.0",
        "nodemon": "^1.18.6",
        "prettier": "^1.16.4",
        "ts-node": "^7.0.1",
        "tslint": "^5.13.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.3.3333"
    }
}
